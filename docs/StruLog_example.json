{
  "time": "LOCAL",
  "usingStores": [ "console", "file" ],
  "insideLoggingStore": "console",
  "stores": {
    "console": {
      "outputPattern": "{time} | {loggerName} | {msg} {obj} {excClassLine} {excMsg} {excStackTrace}",
      "minLogLevel": "Trace"
    },
    "file": {
      "path": "{project}/Logs/{y}/{m}/StruLogTest-{d}.log",
      "outputPattern": "{time} | {logLevel} | {loggerName} | {msg} {obj} {excClassLine} {excMsg} {excStackTrace}",
      "minLogLevel": "Trace",
      "recreationPeriodInDays": 1
    },
    "mongoDB": {
      "connectionString": "mongodb://127.0.0.1:27017/?compressors=disabled&gssapiServiceName=mongodb",
      "collectionName": "Logs_StruLogTest",
      "dbName": "Steam",
      "outputPattern": "{time} {logLevel} {loggerName} {msg} {obj} {excClassLine} {excMsg} {excStackTrace}",
      "minLogLevel": "Info"

    }
  }
}